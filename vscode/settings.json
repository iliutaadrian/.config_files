{
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
    "terminal.external.osxExec": "",
    "editor.accessibilitySupport": "off",
    "editor.stickyScroll.enabled": false,
    "editor.suggest.insertMode": "replace",
    "editor.linkedEditing": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "launch": {},
    "[json]": {},
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": true,
    "workbench.iconTheme": "material-icon-theme",
    "update.showReleaseNotes": false,
    "zenMode.hideLineNumbers": false,
    "editor.lineNumbers": "relative",
    "vim.useSystemClipboard": true,
    "vim.easymotion": true,
    "vim.leader": "<Space>",
    "vim.hlsearch": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                "/"
            ],
            "commands": [
                "editor.action.commentLine",
                "editor.editorTextFocus"
            ]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "<C-h>"
            ],
            "commands": [
                "cursorLeft"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "commands": [
                "cursorDown"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "commands": [
                "cursorUp"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "commands": [
                "cursorRight"
            ]
        },
    ],
    "terminal.integrated.profiles.osx": {
        "tmux-shell": {
            "path": "tmux",
            "args": [
                "new-session",
                "-A",
                "-s",
                "${workspaceFolderBasename}"
            ]
        }
    },
    "terminal.integrated.defaultProfile.osx": "tmux-shell",
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "projectManager.git.baseFolders": [
        "",
        "/Users/iliutaadrian/Sites"
    ],
    "codeium.enableExplainProblem": false,
    "codeium.enableCodeLens": false,
    "rubocop.mode": "enableUnconditionally",
    "editor.wordWrap": "on",
    "codeium.enableCommandSideHintEducation": false,
    "errorLens.statusBarIconsAtZero": "hide",
    "errorLens.gutterIconsEnabled": true,
    "errorLens.messageBackgroundMode": "message",
    "workbench.colorCustomizations": {
        "editorError.foreground": "#00000000",
        "editorWarning.foreground": "#00000000",
        "editorInfo.foreground": "#00000000"
    },
    "[ruby]": {
        "editor.defaultFormatter": "rubocop.vscode-rubocop"
    },
    "formatFiles.extensionsToInclude": "rb",
    "formatFiles.excludePattern": "*",
    "formatFiles.excludedFolders": [
        "node_modules",
        ".vscode",
        ".git",
        "dist",
        ".chrome",
        "db"
    ],
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true, // enable for all themes
        "[Default Dark+]": {
            "enabled": true // enable for a particular theme
        }
    },
    "workbench.colorTheme": "Ruby Steel Blue",
    "gitlens.currentLine.enabled": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    "workbench.editor.limit.value": 4,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.excludeDirty": true,
    "vim.foldfix": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "codeium.enableSearch": true,
    "jump.primaryCharset": "fsdghjklweruioncv",
    "ruby.rubocop.onSave": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "workbench.startupEditor": "none",
}